{
  "name": "patriotpoll",
  "version": "0.7.11",
  "main": "./index.js",
  "unpkg": "./dist/PatriotPoll.umd.min.js",
  "scripts": {
    "serve": "npx vue-cli-service serve",
    "build": "npm run build-lib",
    "build-lib": "npx vue-cli-service build --inline-vue --target lib --name PatriotPoll index.js",
    "build-wc": "npx vue-cli-service build --inline-vue --target wc --name patriot-poll index.js",
    "lint": "npx vue-cli-service lint",
    "fix": "npx vue-cli-service lint --fix",
    "pre-release": "git add . -A; git commit -m 'pre-release commit'; git pull origin master;",
    "release-patch": "npm version patch && npm run release;",
    "release-minor": "npm version minor && npm run release;",
    "release-major": "npm version major && npm run release;",
    "release": "git push --follow-tags origin master"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",

    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/vue-fontawesome": "^2.0.0",
    "@vue-interface/activity-indicator": "^0.10.18",
    "@vue-interface/alert": "^1.0.26",
    "@vue-interface/animate-css": "^0.9.8",
    "@vue-interface/btn": "^0.9.11",
    "@vue-interface/btn-activity": "^0.9.8",
    "@vue-interface/btn-group": "^0.9.3",
    "@vue-interface/date-filter": "^0.8.2",
    "@vue-interface/form-control": "^0.10.1",
    "@vue-interface/input-field": "^0.11.0",
    "@vue-interface/lazy": "^1.0.1",
    "@vue-interface/progress-bar": "^0.9.15",
    "@vue-interface/select-field": "^0.9.0",
    "@vue-interface/slide-deck": "^0.9.8",
    "@vue-interface/utils": "^0.9.33",
    "axios": "^0.21.0",
    "bootstrap": "^4.5.3",
    "chart.js": "^2.9.3",
    "color": "^3.1.3",
    "core-js": "^3.8.1",
    "randomcolor": "^0.6.2",
    "vue": "^2.6.12",
    "vue-social-sharing": "^3.0.2"

    "@vue/cli-service-global": "^4.5.9",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.11.0",
    "eslint-plugin-vue": "^7.0.1",
    "node-sass": "^5.0.0",
    "postcss-prefix-selector": "^1.7.2",
    "sass-loader": "^10.0.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.0"
  },
  "sideEffects": [
    "*.css",
    "*.vue"
  ]
